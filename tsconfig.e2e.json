{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "noUnusedLocals": false,
    "outDir": "out-e2e",
    "target": "es5",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "include": [
    "**/*.e2e-spec.ts",
    "**/*.d.ts"
  ],
  "exclude": [
    "*-SKIP-*",
    "AOT-*",
    "More",
    "SKIP-*",
    "node_modules"
  ],
  "extends": "./tsconfig.json"
}
